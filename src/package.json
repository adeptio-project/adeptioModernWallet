{
  "name": "AdeptioModernWallet",
  "version": "0.3.0",
  "description": "Desktop wallet for Adeptio currency",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-darwin": "electron-packager . adeptioModernWallet --platform darwin --arch x64 --overwrite --icon=assets/images/icon.ico --prune=true --out dist/ --asar",
    "build-linux": "electron-packager . adeptioModernWallet --platform linux --arch x64 --overwrite --icon=assets/images/icon.ico --prune=true --out dist/ --icon assets/images/icon.ico --asar",
    "build-windows": "electron-packager . adeptioModernWallet --platform win32 --arch x64 --overwrite --icon=assets/images/icon.ico --prune=true --out dist/ --asar"
  },
  "repository": "https://github.com/adeptio-project",
  "keywords": [
    "Adeptio",
    "adeptiomodernwallet",
    "storade"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^5.0.2",
    "electron-installer-dmg": "^2.0.0",
    "electron-installer-snap": "^3.2.0",
    "electron-winstaller": "^3.0.4"
  },
  "dependencies": {
    "after-load": "1.0.4",
    "app-root-path": "^2.2.1",
    "bitcoin-core": "2.0.0",
    "fs-extra": "^8.1.0",
    "qrcode": "^1.3.3",
    "request": "2.88.0",
    "str": "^1.2.2",
    "xmlhttprequest": "^1.8.0"
  }
}

